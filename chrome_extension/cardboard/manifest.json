{
  "manifest_version": 3,
  "name": "Cardboard Chrome Extension",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabGroups",
	"cookies"
  ],
  "host_permissions": [
	"http://localhost:8000/*"
  ]
}	